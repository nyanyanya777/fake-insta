
<%= form_for(@photos, url: choose_new_or_edit) do |f| %>
<%= f.label :タイトル %>
<%= f.text_field :title, class: 'form-control'%>
<div class="field">
  <div class="text_field">
<div class = form>
<%= f.label :内容 %>
<%= f.text_field :content, class: 'form-control' %>
</div>
</div>
<br>
<%= f.submit '投稿', class: 'btn btn-primary btn-block' %>
<% end %>
